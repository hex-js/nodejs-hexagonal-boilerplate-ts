{
  "name": "nodejs-hexagonal-boilerplate-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --extensions \".js,.ts,.json\" --out-dir dist --copy-files --no-copy-ignored",
    "watch": "babel src --watch --extensions \".js,.ts,.json\" --out-dir dist --copy-files --no-copy-ignored",
    "depcheck": "depcheck",
    "test-ci": "cross-env ENV_NAME=test jest --ci --reporters=jest-junit --outputFile=./test-results/jest",
    "start": "node dist/ports/http/server",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/ports/http/server.ts",
    "test": "cross-env ENV_NAME=test jest --verbose --runInBand",
    "lint": "eslint -c .eslintrc.json src/**/*.ts",
    "local-lambda": "serverless offline -s dev",
    "prebuild-lambda": "yarn build",
    "build-lambda": "serverless package",
    "stryker-mutate": "stryker run"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "./test-results/jest",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "engines": {
    "node": "^12.18.4",
    "yarn": "^1.20.0",
    "npm": "^6.10.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.754.0",
    "cuid": "^2.1.8",
    "debug": "^4.1.1",
    "escriba": "^2.10.0",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "moment": "^2.28.0",
    "moment-timezone": "^0.5.31",
    "ramda": "^0.27.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@stryker-mutator/core": "^3.3.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/moment-timezone": "^0.5.30",
    "@types/ramda": "0.27.17",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitlint": "^11.0.0",
    "cross-env": "^7.0.2",
    "depcheck": "^1.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "lint-staged": "^10.4.0",
    "prettier-eslint": "11.0.0",
    "rimraf": "^3.0.2",
    "serverless": "^2.1.1",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-jetpack": "^0.10.7",
    "serverless-localstack": "^0.4.27",
    "serverless-offline": "^6.7.0",
    "standard": "^14.3.4",
    "ts-jest": "^26.4.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0"
  },
  "resolutions": {
    "ini": ">1.3.6"
  }
}
